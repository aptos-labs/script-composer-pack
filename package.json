{
  "name": "@aptos-labs/script-composer-pack",
  "collaborators": [
    "Aptos Labs <opensource@aptoslabs.com>"
  ],
  "description": "Generating Move Script from composer",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptos-labs/aptos-core"
  },
  "files": [
    "dist",
    "main.mts",
    "tsconfig.json",
    "tsup.config.ts"
  ],
  "main": "dist/cjs/main.js",
  "module": "dist/esm/main.mjs",
  "types": "dist/main.d.ts",
  "homepage": "https://aptoslabs.com",
  "sideEffects": [
    "./snippets/*"
  ],
  "devDependencies": {
    "@rollup/plugin-wasm": "6.2.2",
    "@types/node": "^22.15.17",
    "buffer": "6.0.3",
    "esbuild": "0.25.4",
    "fflate": "0.8.2",
    "tsc": "2.0.4",
    "tsup": "8.4.0",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/esm/main.mjs"
      },
      "default": {
        "types": "./dist/main.d.ts",
        "default": "./dist/cjs/main.js"
      }
    }
  },
  "scripts": {
    "build": "npx tsup",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": "<4.0.0",
    "@aptos-labs/aptos-dynamic-transaction-composer": "0.1.4-0"
  }
}
